						<button class="choice" data-value="holdings">Holdings</button>
						<button class="choice" data-value="activity">Activity</button>
						<button class="choice" data-value="settings">Settings</button>
					</div>
				</div>
				<div class="section">
					<div class="top noselect">
						<span class="title">Holdings</span>
					</div>
					<div class="bottom">
						<button class="submit inline" id="import-holdings-button">Import Holdings</button>
						<button class="submit inline" id="export-holdings-button">Export Holdings</button>
					</div>
				</div>
				<div class="section">
					<div class="top noselect">
						<span class="title">Activity</span>
					</div>
					<div class="bottom">
						<button class="submit inline" id="import-activity-button">Import Activity</button>
						<button class="submit inline" id="export-activity-button">Export Activity</button>
					</div>
				</div>
				<div class="section">
					<div class="top noselect">
						<span class="title">Historical Data</span>
					</div>
