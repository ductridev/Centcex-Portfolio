						<button class="choice" data-value="gbp">GBP</button>
						<button class="choice" data-value="eur">EUR</button>
						<button class="choice" data-value="chf">CHF</button>
						<button class="choice" data-value="aud">AUD</button>
						<button class="choice" data-value="jpy">JPY</button>
						<button class="choice" data-value="cad">CAD</button>
